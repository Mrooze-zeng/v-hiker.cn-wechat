/**app.wxss**/
image {
    width: 100%;
    height: 100%;
}
.slide-image {
    width: 100%;
    height: 100%;
}
contact-button {
    /*width: 60px;
    height: 23px;*/
    background: #09bb07;
    position: fixed;
    padding:15rpx;
    border-radius:20rpx;
    font-size:12pt;
    color:#fff;
}
.status-danger {
    width:120rpx;
    height:105rpx;
    border:4rpx solid #e64340;
    position:absolute;
    top:40rpx;
    right:120rpx;
    border-radius:60rpx;
    font-size:12pt;
    line-height:45rpx;
    text-align:center;
    padding-top:15rpx;
    transform:rotateZ(-45deg);
}
.myIcons {
    width: 60rpx;
    height: 60rpx;
}
.bg {
    width: 100%;
    height: 100%;
    background: #fff;
}